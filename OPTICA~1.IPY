  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.linear_model import LogisticRegression\n",
    "x = train_set[:,0:64]      \n",
    "y = train_labels             \n",
    "lr = LogisticRegression(max_iter = 3000)\n",
    "lr.fit(x,y)\n",
    "y_pred = lr.predict(test_set)\n",
    "preds = y_pred"
   ]
